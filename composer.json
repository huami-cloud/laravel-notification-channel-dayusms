{
    "name": "huami-cloud/laravel-notification-channel-dayusms",
    "description": "Alibaba Dayu SMS Channel for Laravel 5.3",
    "homepage": "https://www.alidayu.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "HFEx",
            "email": "hfex@huami.com",
            "homepage": "http://www.huami.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "huami-cloud/taobao-php-sdk": "1.0.*",
        "illuminate/notifications": "5.3.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "orchestra/testbench": "^3.3@dev",
        "phpunit/phpunit": "4.*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Dayusms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Dayusms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
